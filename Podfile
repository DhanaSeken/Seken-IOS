# Uncomment the next line to define a global platform for your project
 platform :ios, ‘8.0’

target 'Seken' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
 use_frameworks!
  pod 'Google/SignIn'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'

  # Pods for Seken

  target 'SekenTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SekenUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
